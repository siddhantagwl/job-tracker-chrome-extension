{
  "manifest_version": 3,
  "name": "Job Tracker",
  "version": "0.1.0",
  "description": "Quickly saves job information to a Google Sheet.",
  "permissions": [
    "activeTab",
    "storage"
  ],
  "host_permissions": ["https://script.google.com/*"],
  "action": {
    "default_popup": "popup.html",
    "default_title": "Save Job",
    "default_icon": "icon48.png"
  }
}
